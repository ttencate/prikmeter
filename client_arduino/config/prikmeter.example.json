{
  /* The SSID (network name) and password of your WiFi router. */
  "wifiSsid": "your_wifi_ssid",
  "wifiPassword": "your_wifi_password",
  /* The fully qualified domain name of your server. */
  "serverHost": "upload.prikmeter.example.com",
  /* The fingerprint of the SSL certificate used by your server, in a format like
  this: "25:09:FB:22:F7:67:1A:EA:2D:0A:28:AE:80:51:6F:39:0D:E0:CA:21" */
  "serverCertificateFingerprint": "your_certificate_fingerprint",
  /* The authentication token associated on the server with your account. */
  "authToken": "your_auth_token"
}
